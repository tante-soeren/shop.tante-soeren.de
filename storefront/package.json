{
  "name": "vendure-angular-storefront",
  "version": "0.1.10",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201",
    "build": "ng build",
    "analyze-prod-bundle": "ng build --prod --statsJson=true && webpack-bundle-analyzer dist/browser/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate-types": "gql-gen --config codegen.yml",
    "dev:ssr": "ng run reference-storefront:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run reference-storefront:server:production",
    "prerender": "ng run reference-storefront:prerender",
    "translate": "ng xi18n --output-path translate/ --out-file messages.en.xlf --i18n-locale en-US"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.2",
    "@angular/cdk": "^9.0.1",
    "@angular/common": "^9.0.2",
    "@angular/compiler": "^9.0.2",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^9.0.2",
    "@angular/localize": "^9.1.2",
    "@angular/platform-browser": "^9.0.2",
    "@angular/platform-browser-dynamic": "^9.0.2",
    "@angular/platform-server": "^9.0.2",
    "@angular/router": "^9.0.2",
    "@angular/service-worker": "^9.0.2",
    "@fortawesome/angular-fontawesome": "^0.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@nguniversal/express-engine": "^9.0.0",
    "apollo-angular": "^1.5.0",
    "apollo-angular-link-http": "^1.5.0",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.13",
    "bootstrap": "^4.3.1",
    "compression": "^1.7.4",
    "cookie-notice": "^1.2.4",
    "cookieconsent": "^3.1.1",
    "core-js": "^3.1.3",
    "express": "^4.15.2",
    "global": "^4.4.0",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "ngx-cookieconsent": "^2.2.3",
    "photoswipe": "^4.1.3",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.3",
    "@angular-devkit/build-ng-packagr": "~0.900.3",
    "@angular/cli": "^9.1.1",
    "@angular/compiler-cli": "^9.0.2",
    "@angular/language-service": "^9.0.2",
    "@nguniversal/builders": "^9.0.0",
    "@types/compression": "^0.0.36",
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.0.5",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^12.11.1",
    "@types/photoswipe": "^4.1.0",
    "codelyzer": "^5.1.2",
    "graphql-code-generator": "0.18.0",
    "graphql-codegen-add": "0.18.0",
    "graphql-codegen-typescript-client": "0.18.0",
    "graphql-codegen-typescript-common": "0.18.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
